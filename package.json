{
  "name": "base-node-ts",
  "version": "0.3.1",
  "description": "NodeJS base project with TypeScript for the rapid creation of backends with certain functionalities already created and programming patterns already defined.",
  "main": "index.js",
  "scripts": {
    "start": "node build/index.js",
    "dev": "ts-node-dev -r tsconfig-paths/register --ignore-watch node_modules src/index.ts",
    "build": "tsc && tsc-alias",
    "test": "echo \"Error: no test specified\" && exit 1",
    "commit": "git add . && npx gitmoji -c",
    "format": "npx prettier --write .",
    "format-check": "npx prettier --check .",
    "generate-token-keys": "sh generate-token-keys.sh",
    "generate-token-keys:local": "sh generate-token-keys.sh local"
  },
  "keywords": [
    "mvc",
    "node",
    "nodejs",
    "typescript",
    "eslint",
    "prettier",
    "mongodb",
    "jsonwebtoken",
    "api versioning",
    "swagger documentation",
    "path alias",
    "mit"
  ],
  "author": {
    "name": "Luis Fernando Solano Mart√≠nez",
    "email": "luisfer.sm15@gmail.com",
    "url": "https://github.com/XxLuisFer15xX"
  },
  "license": "MIT",
  "dependencies": {
    "@sinclair/typebox": "0.24.51",
    "ajv": "8.11.0",
    "ajv-errors": "3.0.0",
    "ajv-formats": "2.1.1",
    "basic-auth": "2.0.1",
    "bcrypt": "5.1.0",
    "cors": "2.8.5",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "jsonwebtoken": "8.5.1",
    "mongoose": "6.7.0",
    "morgan": "1.10.0",
    "swagger-ui-express": "4.5.0",
    "yamljs": "0.3.0"
  },
  "devDependencies": {
    "@types/basic-auth": "1.1.3",
    "@types/bcrypt": "5.0.0",
    "@types/cors": "2.8.13",
    "@types/express": "4.17.14",
    "@types/jsonwebtoken": "8.5.9",
    "@types/morgan": "1.9.3",
    "@types/swagger-ui-express": "4.1.3",
    "@types/yamljs": "0.2.31",
    "@typescript-eslint/eslint-plugin": "5.41.0",
    "eslint": "8.26.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-standard-with-typescript": "23.0.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-n": "15.3.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.1.1",
    "prettier": "2.7.1",
    "ts-node-dev": "2.0.0",
    "tsc-alias": "1.7.0",
    "tsconfig-paths": "4.1.0",
    "typescript": "4.8.4"
  }
}
